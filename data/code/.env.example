# 청년정책 데이터베이스 환경변수 설정

# OpenAI API 키 (임베딩 생성용)
OPENAI_API_KEY=your_openai_api_key_here

# PostgreSQL 데이터베이스 설정
DB_HOST=localhost
DB_NAME=youth_policy
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_PORT=5432

# 임베딩 설정
EMBEDDING_MODEL=text-embedding-3-large
EMBEDDING_DIMENSION=3072
BATCH_SIZE=100
